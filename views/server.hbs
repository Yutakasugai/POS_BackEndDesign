<div class="orderBtn-modal" id="orderBtn-modal">

    <div class="modal-content-orderBtn">

        <span class="close-btn-orderBtn">&times;</span>

        <div class="orderBtn-title">
            <h1>Coming Items</h1>
        </div>

        <div class="orderBtn-textBox">
            <div class="scrollable-orders">
                <div class="review-container">
                    <table border cellspacing="0" class="review-table">
                        {{#each items}}
                            <tr class="reviewBox-header" name="row_{{kitchen_id}}" id="row_{{id}}">
                                <td class="tableNum-row" get_id="{{id}}">{{table_id}}</td>
                                <td class="order-row">{{item_name}}</td>
                            </tr>
                        {{/each}}
                    </table>

                </div>
            </div>
        </div>
    </div>
</div>  

