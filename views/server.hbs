<!-- Update Modal -->
<div class="updateBtn-modal" id="updateBtn-modal">

    <div class="modal-content-updateBtn">

        <span class="close-btn-updateBtn">&times;</span>

        <div class="table-updateBox">
            <!-- Table Panel -->
            <h1 id="update-number"></h1>

            <!-- Customer Number Panel -->
            <div class="customerCheck">

                <h1>Customer: </h1>

                <div id="button_wrapper">
                    <button id="button_decrease">-</button>
                    <h1 id="counter">0</h1>
                    <button id="button_increase">+</button>
                </div>

            </div>

            <!-- Table Switch Panel -->
            <div class="table-switch">

                <h1>Switch: </h1>

                <div id="s-button_wrapper">
                    <button id="s-button_decrease">-</button>
                    <h4>Table<h1 id="s-counter" style="display: none;">0</h1></h4>
                    <button id="s-button_increase">+</button>
                </div>

            </div>

            <!-- Cancel and Confirm Button -->
            <div class="button-container">
                <button id="cancel-btn">Cancel</button>

                <form action="/auth/user/updateTable" method="POST">
                    <input type="hidden" name="userName" value="{{name}}">
                    <input type="hidden" name="date_key" value="{{Date}}">
                    <input type="hidden" name="time_key" value="{{Time}}">
                    {{!-- <input type="hidden" id="new_c_number" name="new_c_number" value="">
                    <input type="hidden" id="new_table_key" name="new_table_key" value=""> --}}
                    <button id="confirm-btn" name="table_key" value="">Confirm</button>
                </form>
            </div>

        </div>
    </div>
</div>

<!-- addBtn Modal -->
